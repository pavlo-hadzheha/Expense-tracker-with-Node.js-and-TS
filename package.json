  "license": "ISC",
  "dependencies": {
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1",
    "inquirer": "^8.2.6",
    "progress": "^2.0.3",
    "radash": "^11.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.4.10",
    "@types/uuid": "^9.0.2",
    "nodemon": "^3.0.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.2.2"
